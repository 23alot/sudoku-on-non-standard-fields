<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="sudoku.newgame">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GeneratorActivity"></activity>
        <activity android:name=".LeaderboardActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".DimensionActivity"></activity>
        <activity android:name=".BoardsActivity"></activity>
        <activity android:name=".CongratulationActivity"></activity>
        <activity android:name=".StatisticsActivity"></activity>
        <activity android:name=".firebaseauth.ChooserActivity"></activity>
        <activity android:name=".firebaseauth.FirebaseUIActivity"></activity>
        <activity android:name=".firebaseauth.GoogleSignInActivity"></activity>
        <activity android:name=".SettingsActivity"></activity>
        <activity android:name=".TuturuActivity" />
        <activity android:name=".DemoActivity" />
    </application>
</manifest>
