<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="sudoku.newgame">

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GeneratorActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".DimensionActivity"></activity>
        <activity android:name=".BoardsActivity"></activity>
        <activity android:name=".CongratulationActivity"></activity>
        <activity android:name=".StatisticsActivity"></activity>
        <activity android:name=".firebaseauth.ChooserActivity"></activity>
        <activity android:name=".firebaseauth.FirebaseUIActivity"></activity>
        <activity android:name=".firebaseauth.GoogleSignInActivity"></activity>
        <activity android:name=".SettingsActivity"></activity>
    </application>

</manifest>
